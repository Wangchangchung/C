<script type="text/javascript">
	$('li.list-group-item').click(function(){
		if( $(this).hasClass('active') ){
			console.info('do nothing');
		} else {
			console.info('do something');
			$('ul.list-group li.active').removeClass('active');
			$(this).addClass('active');
		}
		
		var name = $(this).attr('name');
		if( name == '1' || name == 'all' ){
			$('div.course_item').css('display','block');
			$('div.no_course').css('display','none');
		} else {
			$('div.course_item').css('display','none');
			$('div.no_course').css('display','block');
		}
		
		$('div.course_type_name').html($(this).html());
	});
</script>
